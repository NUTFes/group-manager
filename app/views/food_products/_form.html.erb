<%= simple_form_for(@food_product) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :group %>
    <%= f.input :name %>
    <%= f.input :num %>
    <%= f.input :is_cooking %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
